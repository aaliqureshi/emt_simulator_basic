   "source": [
    "from sympy import *"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "v1d, v1q, R12, L12, omega, id, iq = symbols('v1d, v1q, R12, L12, omega, id, iq',real=true)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "eq1 = v1d - R12*id + omega*L12*iq\n",
    "\n",
    "eq2 = v1q - R12*iq - omega*L12*id"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "{id: (L12*omega*v1q + R12*v1d)/(L12**2*omega**2 + R12**2),\n",
       " iq: (-L12*omega*v1d + R12*v1q)/(L12**2*omega**2 + R12**2)}"
      ]
     },
     "execution_count": 7,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "solve((eq1,eq2),(id,iq))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
